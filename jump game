bool canJump(int* nums, int numsSize) {
    int maxp = 0;
    int i=0;
    int lasti = numsSize -1;
    
    while(i<numsSize){
        if (i > maxp) {
            return false;
        }
        int maxc = 0;
        maxc = nums[i] + i;

        if(maxp < maxc){
            maxp = maxc;
        }
        if(maxp >= lasti){
            return true;
        }
        i++;
    }
    return false;
}
