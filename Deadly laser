#include<stdio.h>


int main(){
    int n,m,x,y,d;
    
    scanf("%d %d %d %d %d", &n, &m, &x, &y, &d);

    if ((x - d <= 1 && x + d >= n) &&
            (y - d <= 1 && y + d >= m)) {
            printf("-1\n");
        } else {
            printf("%d\n", (n - 1) + (m - 1));
        }

    return 0;
}
